<objects>

        <object name="ИмяОбъекта">
        <field name="ИмяПоля" type="int" />
        <field name="Поле2" type="float" />
        <method name="ИмяМетода" return="float" type="Python">
            <argument name="condition" type="bool" />
            <argument name="value" type="float" />
            <body>
                if condition:
                    return value * value
                else:
                    for _ in range(100): value *= 1.1
                    return value
            </body>
        </method>
        <method name="НайтиПользователя" return="int" type="SQL">
            <argument name="name" type="text"/>
            <body>
                SELECT user.id FROM user WHERE user.login = :name;
            </body>
        </method>
    </object>

